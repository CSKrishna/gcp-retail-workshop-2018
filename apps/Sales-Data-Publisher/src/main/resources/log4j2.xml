<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="INFO" name="Backend" packages="">

    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%5p %d{ISO8601} [%t][%x] %c - %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
       <Root level="INFO">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>

</Configuration>
